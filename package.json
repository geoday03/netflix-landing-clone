    "@next/font": "^13.5.4",
    "framer-motion": "^10.10.0",
    "next": "^13.2.3",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@playwright/test": "^1.31.1",
    "@types/react": "^18.0.31",
    "commitizen": "^4.3.0",
    "commitlint": "^17.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.3",
